                @Html.DisplayFor(modelItem => item.FechaNacimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SpotifyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SpotifyTokenAcceso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SpotifyTokenRefresco)
            </td>
            <td>
